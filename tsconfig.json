{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "types": ["react-scripts"],
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "module": "ESNext",
    "target": "ES6"
  },
  "include": ["src", "src/index.d.ts"]
}
